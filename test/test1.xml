<?xml version="1.0" encoding="utf-8"?>
<rule>
	<name>AppInit registry entry was created [A0101]</name>
	<severity>Warning</severity>
	<Operations>
		<Operation type="RegSetValue">
			<operator type="AND">
                <condition component="RegistryItem" property="Key" condition="ends" value="a" />
                <condition component="RegistryItem" property="Key" condition="ends" value="b" />
			</operator>
		</Operation>
	</Operations>
</rule>

