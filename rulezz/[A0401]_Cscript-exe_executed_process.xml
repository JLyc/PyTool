<?xml version="1.0" encoding="utf-8"?>
<rule> 
	<name>Cscript.exe executed process [A0401]</name>
	<severity>Warning</severity>
	<ParentProcess>
		<Condition component="FileItem" property="Name" condition="is" value="cscript" />
	</ParentProcess>
	<Process>
		<Condition component="FileItem" property="Name" condition="isnotempty" />
	</Process>
</rule>
