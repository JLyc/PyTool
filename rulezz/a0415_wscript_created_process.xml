<?xml version="1.0" encoding="utf-8"?>
<rule> 
	<name>Wscript.exe executed process [A0415]</name>
	<severity>Warning</severity>
	<ParentProcess>
		<Condition component="FileItem" property="Name" condition="is" value="wscript" />
	</ParentProcess>
	<Process>
		<Condition component="FileItem" property="Name" condition="isnot" value="" />
	</Process>
</rule>
