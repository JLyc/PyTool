<?xml version="1.0" encoding="utf-8"?>
<rule>
	<name>AppInit registry entry was created [A0101]</name>
	<severity>Warning</severity>
	<Operations>
		<Operation type="RegSetValue">
			<operator type="OR">
				<operator type="AND">
					<condition component="RegistryItem" property="Key" condition="ends" value="Appinit_Dlls" />
					<operator type="OR">
						<Condition component="RegistryItem" property="Key" condition="contains" value="HKLM\SOFTWARE\Microsoft\Windows NT\CurrentVersion\Windows" />
						<Condition component="RegistryItem" property="Key" condition="contains" value="HKLM\SOFTWARE\Wow6432Node\Microsoft\Windows NT\CurrentVersion\Windows" />
					</operator>
				</operator>
				<operator type="AND">
					<Condition component="RegistryItem" property="Key" condition="ends" value="AppCertDlls" />
					<Condition component="RegistryItem" property="Key" condition="contains" value="HKLM\SYSTEM\ControlSet" />
					<Condition component="RegistryItem" property="Key" condition="contains" value="Control\SESSION MANAGER\" /> 
				</operator>
			</operator>
		</Operation>
	</Operations>
</rule>