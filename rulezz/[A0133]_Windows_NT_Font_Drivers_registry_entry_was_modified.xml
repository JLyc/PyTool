<?xml version="1.0" encoding="utf-8"?>
<rule>
	<name>Windows NT Font Drivers registry entry was modified [A0133]</name>
	<severity>Information</severity>
	<Operations>
		<Operation type="RegSetValue">
				<Condition component="RegistryItem" property="Key" condition="contains" value="SOFTWARE\Microsoft\Windows NT\CurrentVersion\Font Drivers" />
		</Operation>
	</Operations>
</rule>