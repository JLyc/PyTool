<?xml version="1.0" encoding="utf-8"?>
<Rule>
  <Severity>Warning</Severity>
  <Process>
    <operator type="AND">
      <Condition component="LiveGrid" property="Popularity" condition="greater" value="10000" />
      <Condition component="Module" property="OriginalFileName" condition="starts" value="PowerShell" />
      <Condition component="ProcessInfo" property="CommandLineLenght" condition="greater" value="360" />
  </operator>
  </Process>
  <Name>Powershell suspicious activity executed: long cmdline [D415]</Name>
</Rule>
