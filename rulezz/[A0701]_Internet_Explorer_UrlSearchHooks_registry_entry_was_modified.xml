<?xml version="1.0" encoding="utf-8"?>
<rule>
	<name>Internet Explorer UrlSearchHooks registry entry was modified [A0701]</name>
	<severity>Information</severity>
	<Operations>
		<Operation type="RegSetValue">
			<Condition component="RegistryItem" property="Key" condition="contains" value="Software\Microsoft\Internet Explorer\URLSearchHooks" />
		</Operation>
	</Operations>
</rule>