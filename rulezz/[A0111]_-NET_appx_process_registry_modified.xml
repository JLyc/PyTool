<?xml version="1.0" encoding="utf-8"?>
<rule>
	<name>.NET appx_process registry modified [A0111]</name>
	<severity>Warning</severity>
	<Operations>
		<Operation type="RegSetValue">
			<condition component="RegistryItem" property="Key" condition="is" value="HKCU\Environment\APPX_PROCESS" />
		</Operation>
	</Operations>
</rule>