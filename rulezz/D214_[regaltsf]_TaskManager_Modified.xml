<?xml version="1.0" encoding="utf-8"?>
<rule>
   <process>
      <Condition component="LiveGrid" property="Popularity" condition="less" value="1000" />
  </process>
  <Operations>
    <Operation type="RegSetValue">
       <operator type="AND">
           <condition component="RegistryItem" property="Key" condition="contains" value="\Policies\System\DisableTaskMgr" />
        </operator>
     </Operation>
  </Operations>
<name>Unpopular process has enabled\disabled Task Manager [D214]</name>
</rule>