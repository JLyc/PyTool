<?xml version="1.0" encoding="utf-8"?>
<rule> 
	<name>Ntvdm.exe executed process [A0406]</name>
	<severity>Warning</severity>
	<ParentProcess>
		<Condition component="FileItem" property="Name" condition="is" value="ntvdm" />
	</ParentProcess>
	<Process>
		<Condition component="FileItem" property="Name" condition="isnotempty"/>
	</Process>
</rule>
