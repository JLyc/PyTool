<?xml version="1.0" encoding="utf-8"?>
<Rule>
  <Severity>Warning</Severity>
  <Process>
    <operator type="AND">
      <Condition component="LiveGrid" property="Popularity" condition="greater" value="10000" />
      <Condition component="Module" property="OriginalFileName" condition="starts" value="PowerShell" />
      <Condition component="ProcessInfo" property="CommandLine" condition="notcontains" value="\Windows\CCM\SystemTemp\" />
      <Condition component="ProcessInfo" property="CommandLine" condition="contains" value="-E" />
      <operator type="OR">
          <Condition component="ProcessInfo" property="CommandLine" condition="contains" value="-W" />
          <Condition component="ProcessInfo" property="CommandLine" condition="contains" value="-NoL" />
      </operator>
      <operator type="OR">
        <Condition component="ProcessInfo" property="CommandLine" condition="contains" value="-C" />
        <Condition component="ProcessInfo" property="CommandLine" condition="contains" value="-F" />
      </operator>
  </operator>
  </Process>
  <Name>Powershell suspicious activity executed [D413]</Name>
</Rule>